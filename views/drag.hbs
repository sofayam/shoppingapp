<h1> {{title}} </h1>
<script>
  $(function() {

     $(".draggable").draggable()
    $( "#druggable" ).draggable();
    $( "#droppable" ).droppable({
      drop: function( event, ui ) {
        $( this )
          .addClass( "ui-state-highlight" )
          .find( "p" )
            .html( "Dropped!" );
      }
    });
  });
</script>

{{#times 10}} 
<div class="draggable">
  <p>Drag me to my target {{this}}</p>
</div>
{{/times}}
<div id="druggable">
  <p>Drug me to my target </p>
</div>
 
<div id="droppable">
  <p>Drop here</p>
</div>
 

 
