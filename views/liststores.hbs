<h1>{{title}}</h1>

<table>
{{#each	mydata}}
	<tr>
		<td>
			{{this.attributes.name}} 
		</td>
		<td>
			<a href=/chooseloc?id={{this.thingId}}&name={{this.attributes.name}}>locate</a>
		</td>
		<td>
			<a href=/showthing?id={{this.thingId}}>show</a>
		</td>
		<td>
			<a href=/delthing?id={{this.thingId}}>delete</a>
		</td>
		<td>
			{{this.attributes.position.address}}
		</td>
	</tr>
{{/each}}
</table>
